{
    "extends": "./tsconfig.js.json",
    "compilerOptions": {
        "resolveJsonModule": true,
        "noImplicitThis": false,
        "skipLibCheck": true,
    },
    "include": [
        "scripts/*.js",
        "typings/**/*",
        "tests/**/*",
        "typing_tests/**/*",
        "typings/**/*",
        "src/**/*",
        ".eslintrc.js",
        "*.ts",
        "*.js"
    ]
}
