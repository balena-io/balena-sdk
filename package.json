{
  "name": "balena-sdk",
  "version": "15.38.0",
  "description": "The Balena JavaScript SDK",
  "main": "index.js",
  "types": "index.d.ts",
  "browser": "es2015",
  "homepage": "https://github.com/balena-io/balena-sdk",
  "repository": {
    "type": "git",
    "url": "git://github.com/balena-io/balena-sdk.git"
  },
  "files": [
    "es2015/",
    "es2018/",
    "typings/",
    "index.js",
    "index.d.ts",
    "DOCUMENTATION.md"
  ],
  "keywords": [
    "sdk",
    "resin",
    "resin.io",
    "balena",
    "balena.io",
    "iot",
    "devices"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "clean": "rimraf es2015 es2018",
    "test:fast": "npm run lint-fix && npm run build-es2015:fast && npm run test:node",
    "test": "npm run build && npm run test:all",
    "test:all": "npm run test:ts-compatibility && npm run test:dev && npm run test:ts-js && npm run test:typings && npm run test:node && npm run test:browser",
    "test:node": "gulp test",
    "test:browser": "karma start",
    "test:dev": "tsc --noEmit --project ./tsconfig.dev.json",
    "test:ts-js": "tsc --noEmit --project ./tsconfig.js.json",
    "test:ts-compatibility": "npx typescript@~3.9.3 --noEmit --project ./tsconfig.dist.json",
    "test:typings": "tsc --project ./typing_tests/tsconfig.json",
    "build-es2015:fast": "rimraf es2015 && tsc --target es2015 --outDir es2015 && gulp inject-version --buildDir es2015",
    "build-es2015": "npm run build-es2015:fast && gulp pack-browser --buildDir es2015",
    "build-es2018": "rimraf es2018 && tsc --target es2018 --outDir es2018 && gulp inject-version --buildDir es2018 && gulp pack-browser --buildDir es2018",
    "build": "npm run lint && npm run build-es2015 && npm run build-es2018 && npm run docs",
    "docs": "jsdoc2md --template doc/DOCUMENTATION.hbs \"es2015/**/!(balena-browser*.js)\" > DOCUMENTATION.md",
    "ci": "npm test && catch-uncommitted",
    "lint": "balena-lint -e js -e ts --typescript --tests typings lib tests gulpfile.js",
    "lint-fix": "balena-lint -e js -e ts --typescript --fix typings lib tests gulpfile.js",
    "prepublish": "require-npm4-to-publish",
    "prepack": "npm run build",
    "watch": "npm run build && watch \"npm run lint\" \"lib\" \"tests\""
  },
  "author": "Juan Cruz Viotti <juan@balena.io>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0"
  },
  "devDependencies": {
    "@balena/lint": "^5.4.1",
    "@resin.io/types-mochainon": "^2.0.1",
    "@types/mocha": "^5.2.7",
    "@types/ndjson": "^2.0.0",
    "balena-config-karma": "^3.0.0",
    "browserify": "^14.5.0",
    "catch-uncommitted": "^1.6.2",
    "chai-samsam": "0.0.2",
    "dotenv": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-mocha": "^6.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-uglify": "^3.0.2",
    "husky": "^4.3.8",
    "js-polyfills": "^0.1.43",
    "jsdoc-to-markdown": "^5.0.3",
    "karma": "^3.1.4",
    "karma-env-preprocessor": "^0.1.1",
    "lint-staged": "^10.5.4",
    "mocha": "^3.5.3",
    "mocha.parallel": "^0.15.6",
    "mochainon": "^2.0.0",
    "require-npm4-to-publish": "^1.0.0",
    "rimraf": "^3.0.2",
    "rindle": "^1.3.6",
    "superagent": "^3.8.3",
    "tmp": "^0.0.31",
    "ts-node": "^7.0.1",
    "ts-toolbelt": "^6.15.5",
    "typescript": "^4.2.2",
    "uglify-es": "^3.3.9",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@balena/es-version": "^1.0.0",
    "@types/lodash": "^4.14.168",
    "@types/memoizee": "^0.4.5",
    "@types/node": "^10.17.55",
    "abortcontroller-polyfill": "^1.7.1",
    "balena-auth": "^4.1.0",
    "balena-errors": "^4.7.1",
    "balena-hup-action-utils": "~4.0.2",
    "balena-pine": "^12.4.0",
    "balena-register-device": "^7.1.0",
    "balena-request": "^11.4.0",
    "balena-semver": "^2.3.0",
    "balena-settings-client": "^4.0.6",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "moment": "^2.29.1",
    "ndjson": "^2.0.0",
    "semver": "^7.3.4",
    "tslib": "^2.1.0"
  }
}
