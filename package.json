{
  "name": "resin-sdk",
  "version": "7.0.1",
  "description": "The Resin.io JavaScript SDK",
  "main": "build/resin.js",
  "homepage": "https://github.com/resin-io/resin-sdk",
  "repository": {
    "type": "git",
    "url": "git://github.com/resin-io/resin-sdk.git"
  },
  "files": [
    "build/",
    "DOCUMENTATION.md"
  ],
  "keywords": [
    "sdk",
    "resin",
    "resin.io",
    "iot",
    "devices"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "npm run build && npm run test:all",
    "test:all": "npm run test:node && npm run test:browser",
    "test:node": "gulp test",
    "test:browser": "karma start",
    "build": "gulp build && npm run docs",
    "docs": "jsdoc2md \"build/**/!(resin-browser*.js)\" > DOCUMENTATION.md",
    "ci": "npm test && catch-uncommitted",
    "prepublish": "require-npm4-to-publish",
    "prepack": "npm test"
  },
  "author": "Juan Cruz Viotti <juan@resin.io>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0"
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "catch-uncommitted": "^1.0.0",
    "coffee-script": "1.12.5",
    "dotenv": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-coffee": "^2.3.1",
    "gulp-coffeelint": "^0.6.0",
    "gulp-insert": "^0.5.0",
    "gulp-mocha": "^4.1.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.4",
    "js-polyfills": "^0.1.33",
    "jsdoc-to-markdown": "^3.0.0",
    "karma": "^1.3.0",
    "karma-env-preprocessor": "^0.1.1",
    "mocha": "^3.0.0",
    "mochainon": "^1.0.0",
    "request": "^2.67.0",
    "require-npm4-to-publish": "^1.0.0",
    "resin-config-karma": "^1.0.4",
    "rimraf": "^2.6.1",
    "rindle": "^1.2.0",
    "run-sequence": "^1.1.0",
    "superagent": "^3.0.0",
    "tmp": "^0.0.31",
    "vinyl-source-buffer": "^1.1.1"
  },
  "dependencies": {
    "bluebird": "^3.3.1",
    "lodash": "^4.4.0",
    "memoizee": "^0.4.9",
    "moment": "^2.18.1",
    "promise-memoize": "^1.2.0",
    "resin-auth": "^2.0.0",
    "resin-device-logs": "^3.1.0",
    "resin-device-status": "^1.0.1",
    "resin-errors": "^2.10.0",
    "resin-pine": "^6.0.0",
    "resin-register-device": "^4.0.1",
    "resin-request": "^9.0.0",
    "resin-settings-client": "^3.5.2",
    "semver": "^5.3.0"
  }
}
