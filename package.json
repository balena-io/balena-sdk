    "mocha.parallel": "^0.15.6",
    "mockttp": "^3.0.0",
    "os-browserify": "^0.3.0",
    "otplib": "^12.0.1",
    "process": "^0.11.10",
    "rimraf": "^5.0.1",
    "rindle": "^1.3.6",
    "sinon": "^15.1.0",
    "stream-browserify": "^3.0.0",
    "superagent": "^8.0.9",
    "temp": "^0.9.4",
    "tmp": "^0.2.1",
    "ts-node": "^10.9.1",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^5.1.3",
    "uglify-js": "^3.14.1",
    "util": "^0.12.4",
    "vinyl-buffer": "^1.0.1",
