{
  "name": "balena-sdk",
  "version": "14.0.2",
  "description": "The Balena JavaScript SDK",
  "main": "build/balena.js",
  "types": "typings/balena-sdk/index.d.ts",
  "homepage": "https://github.com/balena-io/balena-sdk",
  "repository": {
    "type": "git",
    "url": "git://github.com/balena-io/balena-sdk.git"
  },
  "files": [
    "build/",
    "typings/",
    "DOCUMENTATION.md"
  ],
  "keywords": [
    "sdk",
    "resin",
    "resin.io",
    "balena",
    "balena.io",
    "iot",
    "devices"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "clean": "rimraf build",
    "test:fast": "npm run build:ts && npm run test:node",
    "test": "npm run build && npm run test:all",
    "test:all": "npm run test:ts-compatibility && npm run test:ts-js && npm run test:typings && npm run test:node && npm run test:browser",
    "test:node": "gulp test",
    "test:browser": "karma start",
    "test:ts-js": "npx tsc --noEmit --project ./tsconfig.js.json",
    "test:ts-compatibility": "npx typescript@~3.9.3 --noEmit --project ./tsconfig.dist.json",
    "test:typings": "dtslint --localTs node_modules/typescript/lib --expectOnly typing_tests",
    "build:ts": "tsc && gulp inject-version",
    "build": "npm run lint && npm run clean && npm run build:ts && gulp build && npm run docs",
    "docs": "jsdoc2md --template doc/DOCUMENTATION.hbs \"build/**/!(balena-browser*.js)\" > DOCUMENTATION.md",
    "ci": "npm test && catch-uncommitted",
    "coffeelint": "balena-lint tests",
    "tslint": "balena-lint -e js -e ts --typescript --fix typings lib tests gulpfile.js",
    "lint": "npm run coffeelint && npm run tslint",
    "prepublish": "require-npm4-to-publish",
    "prepack": "npm run build",
    "watch": "npm run build && watch \"npm run lint\" \"lib\" \"tests\""
  },
  "author": "Juan Cruz Viotti <juan@balena.io>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0"
  },
  "devDependencies": {
    "@balena/lint": "^5.0.0",
    "@resin.io/types-mochainon": "^2.0.1",
    "@types/mocha": "^5.2.7",
    "balena-config-karma": "^3.0.0",
    "browserify": "^14.5.0",
    "catch-uncommitted": "^1.6.2",
    "chai-samsam": "0.0.2",
    "coffeescript": "~1.12.7",
    "dotenv": "^4.0.0",
    "dtslint": "^3.4.1",
    "gulp": "^4.0.2",
    "gulp-mocha": "^6.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-uglify": "^3.0.2",
    "husky": "^4.2.5",
    "js-polyfills": "^0.1.42",
    "jsdoc-to-markdown": "^5.0.3",
    "karma": "^3.1.4",
    "karma-env-preprocessor": "^0.1.1",
    "lint-staged": "^10.1.6",
    "mocha": "^3.5.3",
    "mochainon": "^2.0.0",
    "require-npm4-to-publish": "^1.0.0",
    "rimraf": "^3.0.2",
    "rindle": "^1.3.6",
    "superagent": "^3.8.3",
    "tmp": "^0.0.31",
    "ts-node": "^7.0.1",
    "typescript": "^3.9.3",
    "uglify-es": "^3.3.9",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/lodash": "^4.14.150",
    "@types/memoizee": "^0.4.3",
    "@types/node": "^10.17.20",
    "abortcontroller-polyfill": "^1.4.0",
    "balena-auth": "^3.0.1",
    "balena-errors": "^4.4.0",
    "balena-hup-action-utils": "~4.0.1",
    "balena-pine": "^11.0.1",
    "balena-register-device": "^6.1.1",
    "balena-request": "^10.0.9",
    "balena-semver": "^2.3.0",
    "balena-settings-client": "^4.0.4",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.15",
    "memoizee": "^0.4.14",
    "moment": "~2.24.0 || ^2.25.1",
    "ndjson": "^1.5.0",
    "semver": "^7.3.2"
  }
}
